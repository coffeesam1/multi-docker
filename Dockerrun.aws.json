{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions":[
        {
            "name": "client",
            "image": "coffeesam1/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "coffeesam1/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "coffeesam1/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "ngnix",
            "image": "coffeesam1/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings":[{
                "hostPort" : 80,
                "containerPort" : 80
            }],
            "links": ["client", "server"]
        }
    ]
}